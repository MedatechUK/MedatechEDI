            <xs:attribute name="LOCNAME" type="xs:string" use="required" />
            <xs:attribute name="DELVDATE" type="xs:dateTime" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="IVNUM" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
</xs:schema>